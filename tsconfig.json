{
    "compilerOptions": {
      "module": "ES2020",
      "lib": ["DOM", "ES2020"],
      "jsx": "react",
      "target": "ES5",
      "strict": true,
      "jsxFactory": "createElement",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "moduleResolution": "node"
    }
